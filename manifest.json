{
  "manifest_version": 3,
  "name": "YouTube Injector",
  "version": "0.1",
  "description": "Injects a video into YouTube pages. by Tim Voronkin",
  "icons": {
    "48": "icon/icon48.png"
  },
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/"],
      "js": ["src/content.js"]
    }
  ],
  "permissions": ["activeTab"],
  "web_accessible_resources": [
    {
      "resources": ["src/apiKey.txt", "video_IDs.txt"],
      "matches": ["<all_urls>"]
    }
  ]
}
